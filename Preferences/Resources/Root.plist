			<string>Credentials</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>

			<key>defaults</key>
			<string>us.sudhip.stdp</string>

			<key>key</key>
			<string>username</string>

			<key>noAutoCorrect</key>
			<true/>

			<key>placeholder</key>
			<string>Username</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSecureEditTextCell</string>

			<key>defaults</key>
			<string>us.sudhip.stdp</string>

			<key>key</key>
			<string>password</string>

			<key>noAutoCorrect</key>
			<true/>

			<key>placeholder</key>
			<string>Password</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>

			<key>footerText</key>
			<string>Your password will not be shown after it is saved. This is a security feature, not a bug.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>

			<key>label</key>
			<string>Server</string>
