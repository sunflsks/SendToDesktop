		</dict>

        <dict>
			<key>cell</key>
			<string>PSGroupCell</string>

			<key>label</key>
			<string>UI</string>
		</dict>

        <dict>
            <key>cell</key>
            <string>PSSwitchCell</string>

            <key>label</key>
            <string>Enable UI</string>

            <key>default</key>
            <true/>

            <key>key</key>
            <string>enabledui</string>
        </dict>
