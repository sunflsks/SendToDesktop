		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>

			<key>defaults</key>
			<string>us.sudhip.stdp</string>

			<key>key</key>
			<string>directory</string>

			<key>noAutoCorrect</key>
			<true/>

			<key>placeholder</key>
